{
  "name": "react-ts-demo",
  "description": "demonstrates using React with TypeScript, hooks, and more",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "react-scripts build",
    "cover": "open coverage/lcov-report/index.html",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run --browser chrome",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "gentran": "generate-translations",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepush": "npm run verify",
    "start": "react-scripts start",
    "storybook-open": "open http://localhost:9001",
    "storybook-run": "start-storybook -c .storybook -p 9001 -s ./public",
    "storybook": "npm-run-all storybook-open storybook-run",
    "test-no-watch": "cross-env CI=true react-scripts test --env=jsdom",
    "test": "react-scripts test --env=jsdom",
    "verify": "npm-run-all format lint test-no-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.12.12",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-scripts": "2.1.1",
    "typescript": "3.2.2",
    "web-translate": "^0.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0"
  }
}
